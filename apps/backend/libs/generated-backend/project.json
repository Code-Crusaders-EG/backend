{
  "name": "generated-backend",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/backend/libs/generated-backend",
  "projectType": "library",
  "implicitDependencies": [
    "core"
  ],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run proto:build:backend",
        "color": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/backend/libs/generated-backend/package.json"
        ]
      }
    }
  },
  "tags": []
}
