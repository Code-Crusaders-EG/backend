{
  "name": "gateway",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/backend/apps/gateway/src",
  "projectType": "application",
  "implicitDependencies": [
    "users",
    "resources"
  ],
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/backend/apps/gateway",
        "main": "apps/backend/apps/gateway/src/main.ts",
        "tsConfig": "apps/backend/apps/gateway/tsconfig.app.json",
        "assets": [
          "apps/backend/apps/gateway/src/assets"
        ],
        "isolatedConfig": true,
        "webpackConfig": "apps/backend/apps/gateway/webpack.config.js"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "gateway:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "gateway:build:development"
        },
        "production": {
          "buildTarget": "gateway:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/backend/apps/gateway/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/backend/apps/gateway/jest.config.ts"
      }
    }
  },
  "tags": []
}
